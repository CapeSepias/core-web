{
    "name": "dot-rules",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.conf.json --port 3000",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "clean": "rm -rf dist",
        "transpile:dts": "ngc -p ./tsconfig.rules.json",
        "transpile:js": "ngc -p ./tsconfig.rules.allowJs.json",
        "copy-files":
            "copyfiles 'package.json' ./dist/ && cd src && copyfiles './**/*.html', './**/*.scss' './**/*.css' './**/*.jpg' './**/*.gif' './**/*.png' './app/semantic/assets/**/*.*' ../dist/",
        "publish:rules": "npm run bump:version && npm run copy-files && cd dist && npm pack",
        "bump:version":
            "node -e \"var data = require('./package.json');var fs = require('fs');var bump = data.version.split('.');bump[2] = (parseInt(bump[2]) + 1).toString();data.version = bump.join('.');fs.writeFileSync('./package.json', JSON.stringify(data, null, 4));\""
    },
    "dependencies": {
        "@angular/animations": "^6.1.2",
        "@angular/common": "^6.1.2",
        "@angular/compiler": "^6.1.2",
        "@angular/core": "^6.1.2",
        "@angular/forms": "^6.1.2",
        "@angular/http": "^6.1.2",
        "@angular/platform-browser": "^6.1.2",
        "@angular/platform-browser-dynamic": "^6.1.2",
        "@angular/platform-server": "^6.1.2",
        "@angular/router": "^6.1.2",
        "core-js": "^2.5.3",
        "dotcms-js": "file:../dotcms-js/build-dotcms-js/dotcms-js-4.2.0-b70.tgz",
        "primeicons": "^1.0.0-beta.10",
        "primeng": "^6.1.3",
        "rxjs": "^6.3.3",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.7.0",
        "@angular/cli": "^6.1.2",
        "@angular/compiler-cli": "^6.1.2",
        "@angular/language-service": "^6.1.2",
        "@types/googlemaps": "^3.30.11",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "^2.0.3",
        "@types/node": "^6.0.92",
        "codelyzer": "~3.1.1",
        "copyfiles": "^1.2.0",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "^1.7.1",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "^1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.3.2",
        "typescript": "2.9.2"
    },
    "peerDependencies": {
        "@angular/common": "^6.0.0-rc.0 || ^6.0.0",
        "@angular/core": "^6.0.0-rc.0 || ^6.0.0"
    }
}
