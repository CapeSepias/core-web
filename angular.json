{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dotcms-ui": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "baseHref": "./",
            "index": "src/index.html",
            "main": "src/main.ts",
            "outputPath": "dist",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "development": {
              "baseHref": "./",
              "poll": 1000,
              "sourceMap": true,
              "watch": true
            },
            "production": {
              "aot": true,
              "buildOptimizer": true,
              "deleteOutputPath": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dotcms-ui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dotcms-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dotcms-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dotcms-ui-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "dotcms-ui:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dot-layout-grid": {
      "root": "projects/dot-layout-grid",
      "sourceRoot": "projects/dot-layout-grid/src",
      "projectType": "library",
      "prefix": "dot",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/dot-layout-grid/tsconfig.lib.json",
            "project": "projects/dot-layout-grid/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/dot-layout-grid/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dot-layout-grid/src/test.ts",
            "tsConfig": "projects/dot-layout-grid/tsconfig.spec.json",
            "karmaConfig": "projects/dot-layout-grid/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dot-layout-grid/tsconfig.lib.json",
              "projects/dot-layout-grid/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dotcms-js": {
      "root": "projects/dotcms-js",
      "sourceRoot": "projects/dotcms-js/src",
      "projectType": "library",
      "prefix": "dot",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/dotcms-js/tsconfig.lib.json",
            "project": "projects/dotcms-js/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/dotcms-js/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dotcms-js/src/test.ts",
            "tsConfig": "projects/dotcms-js/tsconfig.spec.json",
            "karmaConfig": "projects/dotcms-js/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dotcms-js/tsconfig.lib.json",
              "projects/dotcms-js/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dot-rules": {
      "root": "projects/dot-rules",
      "sourceRoot": "projects/dot-rules/src",
      "projectType": "library",
      "prefix": "dot",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/dot-rules/tsconfig.lib.json",
            "project": "projects/dot-rules/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dot-rules/src/test.ts",
            "tsConfig": "projects/dot-rules/tsconfig.spec.json",
            "karmaConfig": "projects/dot-rules/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dot-rules/tsconfig.lib.json",
              "projects/dot-rules/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dotcms": {
      "root": "projects/dotcms",
      "sourceRoot": "projects/dotcms/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/dotcms/tsconfig.lib.json",
            "project": "projects/dotcms/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dotcms/src/test.ts",
            "tsConfig": "projects/dotcms/tsconfig.spec.json",
            "karmaConfig": "projects/dotcms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dotcms/tsconfig.lib.json",
              "projects/dotcms/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dot-keyvalue": {
      "root": "projects/dot-keyvalue/",
      "sourceRoot": "projects/dot-keyvalue/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist-lib/dot-keyvalue",
            "index": "projects/dot-keyvalue/src/index.html",
            "main": "projects/dot-keyvalue/src/main.ts",
            "polyfills": "projects/dot-keyvalue/src/polyfills.ts",
            "tsConfig": "projects/dot-keyvalue/tsconfig.app.json",
            "assets": [
              "projects/dot-keyvalue/src/favicon.ico",
              "projects/dot-keyvalue/src/assets"
            ],
            "styles": [
              "projects/dot-keyvalue/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/dot-keyvalue/src/environments/environment.ts",
                  "with": "projects/dot-keyvalue/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dot-keyvalue:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dot-keyvalue:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dot-keyvalue:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dot-keyvalue/src/test.ts",
            "polyfills": "projects/dot-keyvalue/src/polyfills.ts",
            "tsConfig": "projects/dot-keyvalue/tsconfig.spec.json",
            "karmaConfig": "projects/dot-keyvalue/karma.conf.js",
            "styles": [
              "projects/dot-keyvalue/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/dot-keyvalue/src/favicon.ico",
              "projects/dot-keyvalue/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dot-keyvalue/tsconfig.app.json",
              "projects/dot-keyvalue/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dot-keyvalue-e2e": {
      "root": "projects/dot-keyvalue-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/dot-keyvalue-e2e/protractor.conf.js",
            "devServerTarget": "dot-keyvalue:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dot-keyvalue:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/dot-keyvalue-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "dotcms-ui",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "dot",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "dot"
    }
  }
}